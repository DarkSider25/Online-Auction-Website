<div class="box">

<li>
<% @msgarr.each do |x| %>
<ul class="msg-txt" > <%= x %> </ul>

<% end %>
</li>
</div>



<%= form_tag action:"register",method:"post"  do %>

<%= label_tag :fname,"First Name:" %>
<%= text_field_tag :fname    %>
</br>

<%= label_tag :lname,"Last Name:" %>
<%= text_field_tag :lname    %>

</br>
<%= label_tag :addr,"Address:" %>
<%= text_area_tag :addr   %>

</br>

<%= label_tag :mobno,"Mobile No:" %>
<%= text_field_tag :mobno    %>
</br>
<%= label_tag :email,"Email:"%>
<%= email_field_tag :email   %>

</br>
<%= label_tag :pwd,"Password:"   %>
<%= password_field_tag :pwd  %>
</br>
         
<%= label_tag :cpwd,"Confirm Password:" %>
<%= password_field_tag :cpwd    %>
 
</br>

<%= submit_tag "Register"  %>
<% end %>WS
